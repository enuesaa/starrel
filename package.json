{
  "name": "@enuesaa/starrel",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "ng serve --port 3000",
    "build:local": "ng build -c local",
    "build:prod": "ng build -c prod",
    "test": "ng test",
    "format": "prettier --write src/*"
  },
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.3",
    "@auth0/auth0-angular": "^2.4.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "@azure/static-web-apps-cli": "^2.0.7",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "endOfLine": "lf",
    "trailingComma": "es5",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "browser-tabs-lock",
      "esbuild",
      "lmdb"
    ]
  }
}
